    <div>
        @RenderBody()
    </div>

    <section id="bottom">
        <div class="container wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="600ms">
            <div class="row">
                @{var footerlist = db.guanwang_footer.Where(c => c.type == 0).ToList();}
                @foreach (var item in footerlist)
                {
                    <div class="col-md-3 col-sm-6">
                        <div class="widget">
                            <h3>@item.name</h3>
                            <ul>
                                @{var list1 = db.guanwang_footer.Where(c => c.type == item.footerID).ToList();}
                                @foreach (var item1 in list1)
                                {
                                    <li><a href="@Server.UrlDecode(item1.url)" target="_blank">@item1.name</a></li>
                                }
                            </ul>
                        </div>
                    </div>
                }
            </div>
        </div>
    </section>
    <!--/#bottom-->

    <footer id="footer" class="midnight-blue">
        <div class="container">
            <div class="row">
                <div class="col-sm-6">
                    &copy; 2016  ll Rights Reserved.
                </div>
                <div class="col-sm-6">
                    <ul class="pull-right">
                        <li><a href="/home/index.shtml">首页</a></li>
                        <li><a href="/home/aboutus.shtml">关于我们</a></li>
                        <li><a href="#">Faq</a></li>
                        <li><a href="/home/contactus.shtml">联系我们</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>
