@{
    ViewBag.Title = "专业网站开发，一直以周期快、质量好、服务好为主旨，开发出您心目中最满意的产品";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

@{List<Model.guanwang_banner_index> bannerlist = ViewData["bannerlist"] as List<Model.guanwang_banner_index>; Model.guanwang_title titlemodel = ViewData["model"] as Model.guanwang_title;}
<section id="main-slider" class="no-margin">
    <div class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            @if (bannerlist != null)
            {
                foreach (var item in bannerlist)
                {
                <li data-target="#main-slider" data-slide-to="@bannerlist.IndexOf(item)" class="@(bannerlist.IndexOf(item) == 0 ? "active" : "")"></li>
                }
            }
        </ol>
        <div class="carousel-inner" style="height:730px;">
            @if (bannerlist != null)
            {
                foreach (var item in bannerlist)
                {
                <div class="item @(bannerlist.IndexOf(item) == 0 ? "active" : "")" style="background-image: url(@item.bannerImage)">
                    <div class="container">
                        <div class="row slide-margin">
                            <div class="col-sm-6">
                                <div class="carousel-content">
                                    <h1 class="animation animated-item-1">@item.bannerTitle</h1>
                                    <h2 class="animation animated-item-2">@item.bannerDescripe</h2>
                                    <a class="btn-slide animation animated-item-3" href="@Server.UrlDecode(item.link)" target="_blank">阅读全文</a>
                                </div>
                            </div>

                            <div class="col-sm-6 hidden-xs animation animated-item-4">
                                <div class="slider-img">
                                    <img src="@item.peopleImage" class="img-responsive">
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                }
            }
        </div>
    </div>
    <!--/.carousel-->
    <a class="prev hidden-xs" href="#main-slider" data-slide="prev">
        <i class="fa fa-chevron-left"></i>
    </a>
    <a class="next hidden-xs" href="#main-slider" data-slide="next">
        <i class="fa fa-chevron-right"></i>
    </a>
</section>
<!--/#main-slider-->


<section id="feature">
    <div class="container">
        <div class="center wow fadeInDown">
            <h2>@titlemodel.title1</h2>
            <p class="lead">@titlemodel.describe1</p>
        </div>

        <div class="row">
            <div class="features">
                @{List<Model.guanwang_service> servicelist = ViewData["servicelist"] as List<Model.guanwang_service>;}
                @foreach (var item in servicelist)
                {
                    <div class="col-md-4 col-sm-6 wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="600ms">
                        <div class="feature-wrap">
                            <i class="fa @item.iconClass"></i>
                            <h2>@item.title</h2>
                            <h3>@item.describe</h3>
                        </div>
                    </div>
                }
            </div>
            <!--/.services-->
        </div>
        <!--/.row-->
    </div>
    <!--/.container-->
</section>
<!--/#feature-->

<section id="recent-works">
    <div class="container">
        <div class="center wow fadeInDown">
            <h2>@titlemodel.title2</h2>
            <p class="lead">@titlemodel.describe2</p>
        </div>

        <div class="row">
            @{List<Model.guanwang_case> caselist = ViewData["caselist"] as List<Model.guanwang_case>;}
            @foreach (var item in caselist)
            { 
                <div class="col-xs-12 col-sm-4 col-md-3">
                    <div class="recent-work-wrap">
                        <img class="img-responsive" src="@item.image" alt="" style="width:292.5px;height:221.89px;">
                        <div class="overlay">
                            <div class="recent-work-inner">
                                <h3><a href="#" onclick="window.open('@Server.UrlDecode(item.link)')">@item.title</a> </h3>
                                <p>@item.describe</p>
                                <a class="preview" href="@item.image" rel="prettyPhoto"><i class="fa fa-eye"></i>预览</a>
                            </div>
                        </div>
                    </div>
                </div>
            }

        </div>
        <!--/.row-->
    </div>
    <!--/.container-->
</section>
<!--/#recent-works-->

<section id="services" class="service-item">
    <div class="container">
        <div class="center wow fadeInDown">
            <h2>@titlemodel.title3</h2>
            <p class="lead">@titlemodel.describe3</p>
        </div>

        <div class="row">
            @{List<Model.guanwang_good> goodlist = ViewData["goodlist"] as List<Model.guanwang_good>;}
            @foreach (var item in goodlist)
            {
                <div class="col-sm-6 col-md-4">
                    <div class="media services-wrap wow fadeInDown">
                        <div class="pull-left">
                            <img class="img-responsive" src="@item.image">
                        </div>
                        <div class="media-body">
                            <h3 class="media-heading">@item.title</h3>
                            <p>@item.describe</p>
                        </div>
                    </div>
                </div>
            }
        </div>
        <!--/.row-->
    </div>
    <!--/.container-->
</section>
<!--/#services-->

<section id="middle">
    <div class="container">
        <div class="row">
            <div class="col-sm-6 wow fadeInDown">
                <div class="skill">
                    <h2>@titlemodel.title4</h2>
                    <p>@titlemodel.describe4</p>

                    @{List<Model.guanwang_skill> skilllist = ViewData["skilllist"] as List<Model.guanwang_skill>;}
                    @foreach (var item in skilllist)
                    {
                        <div class="progress-wrap">
                            <h3>@item.name</h3>
                            <div class="progress">
                                <div class="progress-bar  color@(skilllist.IndexOf(item) + 1)" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: @(item.percent)%">
                                    <span class="bar-width">@(item.percent)%</span>
                                </div>
                            </div>
                        </div>
                    }
                </div>

            </div>
            <!--/.col-sm-6-->

            <div class="col-sm-6 wow fadeInDown">
                <div class="accordion">
                    <h2>为什么客户喜欢和我们合作?</h2>
                    <div class="panel-group" id="accordion1">
                        @{List<Model.guanwang_like> likelist = ViewData["likelist"] as List<Model.guanwang_like>;}
                        @foreach (var item in likelist)
                        {
                            <div class="panel panel-default">
                                <div class="panel-heading @(likelist.IndexOf(item) == 0 ? "active" : "")">
                                    <h3 class="panel-title">
                                        <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion1" href="#collapseOne@(likelist.IndexOf(item) + 1)">@item.navTitle
                                            <i class="fa fa-angle-right pull-right"></i>
                                        </a>
                                    </h3>
                                </div>

                                <div id="collapseOne@(likelist.IndexOf(item) + 1)" class="panel-collapse collapse @(likelist.IndexOf(item) == 0 ? "in" : "")">
                                    <div class="panel-body">
                                        <div class="media accordion-inner">
                                            @if (!string.IsNullOrEmpty(item.image))
                                            {
                                                <div class="pull-left">
                                                    <img class="img-responsive" src="@item.image">
                                                </div> 
                                            }

                                            <div class="media-body">
                                                @if (!string.IsNullOrEmpty(item.title))
                                                {
                                                    <h4>@item.title</h4>
                                                }
                                                <p>@item.describe</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }

                    </div>
                    <!--/#accordion1-->
                </div>
            </div>

        </div>
        <!--/.row-->
    </div>
    <!--/.container-->
</section>
<!--/#middle-->

<section id="content">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-8 wow fadeInDown">
                <div class="tab-wrap">
                    <div class="media">
                        <div class="parrent pull-left">
                            <ul class="nav nav-tabs nav-stacked">
                                @{List<Model.guanwang_product_type> typelist = ViewData["producttypelist"] as List<Model.guanwang_product_type>;}
                                @foreach (var item in typelist)
                                {
                                    <li class="@(typelist.IndexOf(item) == 0 ? "active" : "")"><a href="#tab@(item.typeID)" data-toggle="tab" class="analistic-0@(item.typeID)">@item.name</a></li>
                                }
                            </ul>
                        </div>

                        <div class="parrent media-body">
                            <div class="tab-content">
                                @{List<Model.guanwang_product> productlist = ViewData["productlist"] as List<Model.guanwang_product>;}
                                @foreach (var item in productlist)
                                {
                                    <div class="tab-pane fade @(productlist.IndexOf(item) == 0 ? "active in" : "")" id="tab@(item.typeID)">
                                        <div class="media">
                                            @if (!string.IsNullOrEmpty(item.image))
                                            {
                                                <div class="pull-left">
                                                    <img class="img-responsive" src="@item.image">
                                                </div>
                                            }
                                            <div class="media-body">
                                                @if (!string.IsNullOrEmpty(item.title))
                                                {
                                                    <h2>@item.title</h2> 
                                                }
                                                <p>@item.describe</p>
                                            </div>
                                        </div>
                                    </div>
                                }
                            </div>
                            <!--/.tab-content-->
                        </div>
                        <!--/.media-body-->
                    </div>
                    <!--/.media-->
                </div>
                <!--/.tab-wrap-->
            </div>
            <!--/.col-sm-6-->

            <div class="col-xs-12 col-sm-4 wow fadeInDown">
                <div class="testimonial">
                    <h2>推荐项目</h2>
                    @{List<Model.guanwang_evaluation> recommendlist = ViewData["peoplelist"] as List<Model.guanwang_evaluation>;}
                    @foreach (var item in recommendlist)
                    {
                        <div class="media testimonial-inner">
                            <div class="pull-left">
                                <img class="img-responsive img-circle" src="@item.image" style="width:66px;height:66px;">
                            </div>
                            <div class="media-body">
                                <p>@item.describe</p>
                                <span><strong>-@(item.name)/</strong> 项目地址 @item.url</span>
                            </div>
                        </div>
                    }

                </div>
            </div>

        </div>
        <!--/.row-->
    </div>
    <!--/.container-->
</section>
<!--/#content-->

<section id="partner">
    <div class="container">
        <div class="center wow fadeInDown">
            <h2>@titlemodel.title5</h2>
            <p class="lead">@titlemodel.describe5</p>
        </div>

        <div class="partners">
            <ul>
                @{List<Model.guanwang_partner> partnerlist = ViewData["partnerlist"] as List<Model.guanwang_partner>;}
                @foreach (var item in partnerlist)
                {
                     <li><a href="@item.url" title="@item.name" style="cursor:pointer;" target="_blank">
                    <img class="img-responsive wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="300ms" src="@item.image" alt="@item.name"></a></li>
                }
            </ul>
        </div>
    </div>
    <!--/.container-->
</section>
<!--/#partner-->

<section id="conatcat-info">
    <div class="container">
        <div class="row">
            <div class="col-sm-8">
                <div class="media contact-info wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="600ms">
                    <div class="pull-left">
                        <i class="fa fa-phone"></i>
                    </div>
                    <div class="media-body">
                        <h2>@titlemodel.title6</h2>
                        <p>@titlemodel.describe6</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--/.container-->
</section>
<!--/#conatcat-info-->


